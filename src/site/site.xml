    xsi:schemaLocation="https://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd">
    <bannerLeft>
        <src>/images/owasp.png</src>
        <href>https://owasp.org/www-project-enterprise-security-api/</href>
